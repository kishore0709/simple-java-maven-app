pipeline {
    agent {
        label 'build'
    }
    stages{

        stage('Checkout Project'){
          steps{
            checkout([$class: 'GitSCM',
                       branches: [[name: "refs/tags/${env.VERSION}" ]],
                       doGenerateSubmoduleConfigurations: false,
                       extensions: [],
                       submoduleCfg: [],
                       userRemoteConfigs: [[credentialsId: "$GIT_CREDENTIALS_ID",
                       url: "$PROJECT_REPO" ]]])
            }
        }
        
    }
}
